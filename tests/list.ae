(use '../std/base.ae')
(use '../std/system.ae')

(let print-list [list] ->
  (let print-list-rec [list] ->
    (if (! (is-empty list))
      (print (head list))
      (if (! (is-empty (tail list)))
        (print ' '))
      (<-> (tail list))))
  (print '[')
  (print-list-rec list)
  (println ']'))

(let list [0 1 2])
(set list (+ -1 list))
(set list (+ list -2))

(println (nth list 2))
(println (nth list 10))
(println (len list))
(println (get-range list 1 4))
(print-list list)
(println list)

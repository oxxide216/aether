(use 'std/core')

(let EOF '\d3')

(let printf [arg] -> import 'printf')

(if (!= current-platform 'web')
  (let input-size [size] -> import 'input-size')
  (let input [] -> import 'input'))

(let get-args [] -> import 'get-args')

(macro print [...args] ->
  (printf [args]))

(macro println [...args] ->
  (printf [args '\n']))

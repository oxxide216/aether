(fun print-list [list]
  (fun print-list-rec [list]
    (if (not (is-empty list))
      (print (head list))
      (if (not (is-empty (tail list)))
        (print ' '))
      (print-list-rec (tail list))))
  (print '[')
  (print-list-rec list)
  (println ']'))

(let list [0 1 2])
(add -1 list)
(add list -2)

(println (nth list 2))
(println (nth list 10))
(println (len list))
(print-list list)
(println list)
